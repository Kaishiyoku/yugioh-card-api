{
    "name": "kaishiyoku/yugioh-card-api",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "ext-pdo": "*",
        "diff/diff": "^3.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files":  [
            "app/helpers/arrEach.php",
            "app/helpers/arrMap.php",
            "app/helpers/arrReduce.php",
            "app/helpers/childrenRemove.php",
            "app/helpers/fetchAllSetLinks.php",
            "app/helpers/fetchCard.php",
            "app/helpers/fetchEnglishCardFields.php",
            "app/helpers/fetchGermanCardFields.php",
            "app/helpers/fetchLinkMonster.php",
            "app/helpers/fetchLocaleSpecificCardFields.php",
            "app/helpers/fetchMonster.php",
            "app/helpers/fetchPendulumMonster.php",
            "app/helpers/fetchRitualMonster.php",
            "app/helpers/fetchSetCards.php",
            "app/helpers/fetchSpell.php",
            "app/helpers/fetchSpellOrTrap.php",
            "app/helpers/fetchSynchroMonster.php",
            "app/helpers/fetchTrap.php",
            "app/helpers/fetchXyzMonster.php",
            "app/helpers/filterInt.php",
            "app/helpers/getExternalContent.php",
            "app/helpers/isSameClass.php",
            "app/helpers/removeWhiteSpaces.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    }
}
